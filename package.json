{
  "name": "acknowledgements",
  "description": "âœ¨ Properly credit deps and assets in your projects in a pretty way.",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clembs/acknowledgements.git"
  },
  "files": [
    "dist/index.js"
  ],
  "keywords": [
    "credit",
    "meta",
    "package-managing"
  ],
  "author": "Clembs <clembs@clembs.xyz>",
  "license": "MIT",
  "scripts": {
    "build": "esbuild --outfile=./dist/index.js --target=es2022 --format=esm --platform=node --sourcemap --bundle src/index.ts --external:undici --external:@paperdave/logger",
    "credit": "npx ack --include-dev --markdown",
    "prepare": "yarn build",
    "postinstall": "yarn credit"
  },
  "dependencies": {
    "undici": "^5.8.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@types/yargs": "^17.0.11",
    "esbuild": "^0.14.54",
    "typescript": "^4.7.4"
  },
  "bin": {
    "ack": "dist/index.js"
  }
}